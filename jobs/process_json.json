{
  "taskGroups": [
    {
      "taskSpec": {
        "environment": {
          "variables": {
            "ML_DATASET_ID": "hg19_250_ml",
            "CLOUDASM_DATASET_ID": "hg19_250_cloudasm"
          }
        },
        "runnables": [
          {
            "container": {
              "imageUri": "us-east1-docker.pkg.dev/hmh-em-deepasm/docker-repo/python:d9efc64",
              "commands": ["python", "/app/process_cloudasm_to_ml_datasets.py"]
            }
          }
        ],
        "computeResource": {
          "cpuMilli": 2000,
          "memoryMib": 13000
        },
        "maxRetryCount": 0,
        "maxRunDuration": "4000s"
      },
      "taskCount": 1,
      "parallelism": 1
    }
  ],
  "allocationPolicy": {
    "instances": [
      {
        "policy": { "machineType": "n1-highmem-2" }
      }
    ]
  },
  "labels": {
    "env": "deployment"
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}
