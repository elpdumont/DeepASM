{
  "taskGroups": [
    {
      "taskSpec": {
        "runnables": [
          {
            "container": {
              "imageUri": "us-east1-docker.pkg.dev/hmh-em-deepasm/docker-repo/python:5cf26ea",
              "commands": ["python", "/app/process_raw_json_to_bq.py"]
            }
          }
        ],
        "computeResource": {
          "cpuMilli": 8000,
          "memoryMib": 64000
        },
        "maxRetryCount": 0,
        "maxRunDuration": "4000s"
      },
      "taskCount": 1,
      "parallelism": 0
    }
  ],
  "allocationPolicy": {
    "instances": [
      {
        "policy": { "machineType": "e2-highmem-8" }
      }
    ]
  },
  "labels": {
    "env": "deployment"
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}
